{
  "compilerOptions": {
    "strict": true,
    "lib": [
      "esnext",
      "dom"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react",
    "allowJs": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "downlevelIteration": true,
  },
  "include":[
    ".eslintrc.js",
    "src/**/*",
    "fonts",
  ],
  "exclude": [
    "dist",
    "node_modules",
  ]
}
